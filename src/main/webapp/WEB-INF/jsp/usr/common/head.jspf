					                $(".square-box2").css("background-image", "url('" + data.pimage + "')");
					 	        },
					 	        error: function(error) {
					 	            console.error("Error during registration:", error);
					 	        }
					 	    });
					 	    
					 	}
				
					 	function changeprofileContent() {
