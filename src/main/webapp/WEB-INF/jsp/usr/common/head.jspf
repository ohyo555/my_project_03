						    		'<canvas id="myCanvas" width="100" height="100"></canvas>' +
						            '<a class="modal_close_btn" onclick="closeModal()">Χ</a>' +
						            '<button class="modal_button" onclick="goBack()">뒤로가기</button>';
						        }
